<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="daftarcardgurRowModel" type="com.application.logicliberators.modules.homepageortu.data.model.DaftarcardgurRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameGurucard"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" style="@style/groupStyleindigo_A200cornerRadius">
        <FrameLayout android:id="@+id/frameStackrectangle134One"
            android:layout_gravity="start"
            android:layout_height="@dimen/_160pxv"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_width="@dimen/_334pxh" style="@style/groupStyleindigo_A200_78cornerRadius">
            <ImageView android:id="@+id/imageRectangle134One"
                android:layout_gravity="start"
                android:layout_height="@dimen/_160pxh"
                android:layout_width="@dimen/_334pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_12pxh}"
                imageRes="@{@drawable/img_rectangle134_gray_101}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle134_gray_101"/>
            <FrameLayout android:id="@+id/frameStacknama"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_138pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_217pxh">
                <LinearLayout android:id="@+id/linearColumnnama"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_102pxh"
                    android:layout_marginStart="@dimen/_102pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtNama"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{daftarcardgurRowModel.txtNama}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_8"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStacknamaTwo"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_138pxv" android:layout_width="@dimen/_217pxh">
                    <FrameLayout android:id="@+id/frameStackstartwo"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_110pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_140pxh">
                        <ImageView android:id="@+id/imageStarTwo"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_112pxh"
                            android:layout_width="@dimen/_144pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_star2"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_star2"/>
                        <LinearLayout android:id="@+id/linearRowimg"
                            android:layout_gravity="end"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <View
                                android:background="@drawable/rectangle_bg_gray_101_radius_40"
                                android:id="@+id/viewImg"
                                android:layout_height="@dimen/_80pxh" android:layout_width="@dimen/_80pxh"/>
                            <LinearLayout
                                android:gravity="center_horizontal"
                                android:id="@+id/linearColumnnamaOne"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_33pxv"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_28pxv"
                                android:layout_weight="0.18"
                                android:layout_width="0dp"
                                android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtNamaOne"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{daftarcardgurRowModel.txtNamaOne}"
                                    style="@style/txtPoppinsmediumitalic12" tools:text="@string/lbl_tik"/>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                    <LinearLayout android:id="@+id/linearColumnnamaTwo"
                        android:layout_gravity="top|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="wrap_content"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtNamaTwo"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{daftarcardgurRowModel.txtNamaTwo}"
                            style="@style/txtPoppinsbold12" tools:text="@string/msg_puspita_sari_de"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnnamaThree"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_52pxv"
                        android:layout_marginEnd="@dimen/_33pxh"
                        android:layout_marginStart="@dimen/_33pxh"
                        android:layout_marginTop="@dimen/_52pxv"
                        android:layout_width="wrap_content"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtNamaThree"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{daftarcardgurRowModel.txtNamaThree}"
                            style="@style/txtPoppinsmediumitalic12" tools:text="@string/lbl_sd_kelas_1_6"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHargaRp"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{daftarcardgurRowModel.txtHargaRp}"
                        style="@style/txtPoppinsbold12" tools:text="@string/lbl_harga_rp"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLanguageOne1"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_20pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{daftarcardgurRowModel.txtLanguageOne1}"
                        style="@style/txtPoppinsbold12" tools:text="@string/lbl_banyak_sesi"/>
                    <LinearLayout android:id="@+id/linearColumnnamaFour"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginEnd="@dimen/_82pxh"
                        android:layout_marginStart="@dimen/_82pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtNamaFour"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{daftarcardgurRowModel.txtNamaFour}"
                            style="@style/txtPoppinsmedium12" tools:text="@string/lbl_180_000"/>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
        <ImageView android:id="@+id/imageStarOne"
            android:layout_gravity="end"
            android:layout_height="@dimen/_158pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="@dimen/_86pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_2pxh}"
            imageRes="@{@drawable/img_star1}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_star1"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLanguageOne2"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_4pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{daftarcardgurRowModel.txtLanguageOne2}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_lihat_lebih_lan"/>
    </FrameLayout>
</layout>
