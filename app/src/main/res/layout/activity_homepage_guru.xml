<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homepageGuruVM" type="com.application.logicliberators.modules.homepageguru.data.viewmodel.HomepageGuruVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnbqpuspitasar"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_102">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <FrameLayout android:id="@+id/frameStackselamatdatang"
                android:layout_height="@dimen/_118pxv" android:layout_width="match_parent">
                <LinearLayout
                    android:id="@+id/linearColumnselamatdatang"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleindigo_3">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtSelamatdatang"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_106pxh"
                        android:layout_marginStart="@dimen/_106pxh"
                        android:layout_marginTop="@dimen/_55pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{homepageGuruVM.homepageGuruModel.txtSelamatdatang}"
                        style="@style/txtPoppinssemibold12_1" tools:text="@string/lbl_selamat_datang"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBqPuspitaSar"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_106pxh"
                    android:layout_marginStart="@dimen/_106pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{homepageGuruVM.homepageGuruModel.txtBqPuspitaSar}"
                    style="@style/txtPoppinsbold12" tools:text="@string/msg_bq_puspita_sar"/>
                <ImageView android:id="@+id/imageFotoProfilGur"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_81pxh"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_81pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_gurules1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_gurules1"/>
            </FrameLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearHomepageguru"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_102">
            <HorizontalScrollView android:fillViewport="true"
                android:id="@+id/horizontalScrollHorizontalscrollkelasberlangsu"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="match_parent" android:scrollbars="none">
                <FrameLayout android:id="@+id/frameStackkelasberlangsu"
                    android:layout_height="@dimen/_93pxv" android:layout_width="@dimen/_329pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtKelasBerlangsu"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{homepageGuruVM.homepageGuruModel.txtKelasBerlangsu}"
                        style="@style/txtPoppinsbold15" tools:text="@string/msg_kelas_berlangsu"/>
                    <LinearLayout android:id="@+id/linearKelasguru"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/linearRowfotoprofil"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:orientation="horizontal" style="@style/groupStyleindigo_100cornerRadius">
                            <ImageView android:id="@+id/imageFotoprofil"
                                android:layout_height="@dimen/_55pxh"
                                android:layout_marginBottom="@dimen/_13pxv"
                                android:layout_marginStart="@dimen/_25pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_width="@dimen/_55pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_fotoprofil_3}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_fotoprofil_3"/>
                            <TextView android:id="@+id/txtNamaGuruSubje"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginEnd="@dimen/_1pxh"
                                android:layout_marginStart="@dimen/_24pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_weight="0.63"
                                android:layout_width="0dp"
                                android:text="@{homepageGuruVM.homepageGuruModel.txtNamaGuruSubje}"
                                style="@style/txtPoppinssemibold12" tools:text="@string/msg_budi_hartanto_b2"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRowfotoprofilOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_14pxh"
                            android:layout_weight="0.5"
                            android:layout_width="0dp"
                            android:orientation="horizontal" style="@style/groupStyleindigo_100cornerRadius">
                            <ImageView
                                android:id="@+id/imageFotoprofilOne"
                                android:layout_height="@dimen/_55pxh"
                                android:layout_marginBottom="@dimen/_13pxv"
                                android:layout_marginStart="@dimen/_25pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_width="@dimen/_55pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_fotoprofil}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_fotoprofil"/>
                            <TextView android:id="@+id/txtNamaOrtuSubje"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginEnd="@dimen/_80pxh"
                                android:layout_marginStart="@dimen/_24pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_weight="0.19"
                                android:layout_width="0dp"
                                android:text="@{homepageGuruVM.homepageGuruModel.txtNamaOrtuSubje}"
                                style="@style/txtPoppinssemibold12" tools:text="@string/msg_sri_irawati_tik2"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </HorizontalScrollView>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnnotifikasi"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleindigo_5">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNotifikasi"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{homepageGuruVM.homepageGuruModel.txtNotifikasi}"
                    style="@style/txtPoppinsextrabold18" tools:text="@string/lbl_notifikasi"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnpembayaranuntu"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleindigo_100_72cornerRadius">
                    <TextView android:gravity="start"
                        android:id="@+id/txtPembayaranuntu"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:text="@{homepageGuruVM.homepageGuruModel.txtPembayaranuntu}"
                        style="@style/txtPoppinsbold12" tools:text="@string/msg_pembayaran_untu"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnandamendapatt"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegray_4">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtAndamendapatt"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:text="@{homepageGuruVM.homepageGuruModel.txtAndamendapatt}"
                        style="@style/txtPoppinsbold12" tools:text="@string/msg_anda_mendapat_t"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyleindigo_1">
            <LinearLayout android:id="@+id/linearRowuser"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_27pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowhome"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.89"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameMingcutehome5"
                        android:layout_height="@dimen/_33pxv"
                        android:layout_width="@dimen/_33pxh" style="@style/groupStyleindigo_2">
                        <ImageView android:id="@+id/imageHome"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_25pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_27pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_home"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                    </FrameLayout>
                    <ImageView android:id="@+id/imageRefresh"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_58pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_27pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_refresh"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_73pxh"
                    android:layout_marginStart="@dimen/_64pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
