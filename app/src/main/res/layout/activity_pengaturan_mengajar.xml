<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pengaturanMengajarVM" type="com.application.logicliberators.modules.pengaturanmengajar.data.viewmodel.PengaturanMengajarVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_22pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_A200">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameButtonback"
                    android:layout_height="@dimen/_10pxv"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_5pxh" style="@style/groupStylecornerRadius_3">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPengaturanMeng"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_weight="0.97"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{pengaturanMengajarVM.pengaturanMengajarModel.txtPengaturanMeng}"
                    style="@style/txtInterbold16" tools:text="@string/msg_pengaturan_meng"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleindigo_A200">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnframe1887"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_100cornerRadius">
                <FrameLayout android:id="@+id/frameStackjudulinput"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_110pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_26pxv" android:layout_width="@dimen/_325pxh">
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinput"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJudulInput"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{pengaturanMengajarVM.pengaturanMengajarModel.txtJudulInput}"
                            style="@style/txtInterbold13" tools:text="@string/msg_jumlah_sesi_per"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumninputtext"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylebluegray_100_a2cornerRadius">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtInputText"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_9pxv"
                                android:layout_marginEnd="@dimen/_18pxh"
                                android:layout_marginStart="@dimen/_18pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{pengaturanMengajarVM.pengaturanMengajarModel.txtInputText}"
                                style="@style/txtInterthin12" tools:text="@string/lbl_jumlah_sesi"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinputOne"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJudulInputOne"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{pengaturanMengajarVM.pengaturanMengajarModel.txtJudulInputOne}"
                            style="@style/txtInterbold13" tools:text="@string/lbl_harga_per_bulan"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_100_a2_radius_8"
                            android:ellipsize="end"
                            android:hint="@string/lbl_harga"
                            android:id="@+id/etFrameOne"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@={pengaturanMengajarVM.pengaturanMengajarModel.etFrameOneValue}"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                    </LinearLayout>
                </FrameLayout>
                <Spinner
                    android:background="@drawable/layer_list_bg_bluegray_100_border_black_900_radius_8"
                    android:gravity="center_horizontal"
                    android:id="@+id/spinnerFrame1887"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                <FrameLayout android:id="@+id/frameStackjudulinputTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_88pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_325pxh">
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinputTwo"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJudulInputTwo"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{pengaturanMengajarVM.pengaturanMengajarModel.txtJudulInputTwo}"
                            style="@style/txtInterbold13" tools:text="@string/msg_sistem_bimbinga"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_100_a2_radius_8"
                            android:ellipsize="end"
                            android:hint="@string/msg_kunjungan_di_t"
                            android:id="@+id/etFrameOneOne"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@={pengaturanMengajarVM.pengaturanMengajarModel.etFrameOneOneValue}"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <Spinner
                        android:background="@drawable/layer_list_bg_bluegray_100_border_black_900_radius_8"
                        android:id="@+id/spinnerFrame1887One"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                </FrameLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSimpan"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_28pxv"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_47pxv"
                    android:layout_width="@dimen/_93pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_22pxh"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_simpan"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_9" tools:text="@string/lbl_simpan"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
