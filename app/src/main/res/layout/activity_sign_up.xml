<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.application.logicliberators.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnmasuk"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_50pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_A200">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowmasuk"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyle">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMasuk"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.34"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtMasuk}"
                    style="@style/txtRobotoromansemibold20" tools:text="@string/lbl_masuk"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAppName"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_95pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.66"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtAppName}"
                    style="@style/txtInterbold20" tools:text="@string/lbl_daftar_akun"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearSignup"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleindigo_A200">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnframeten"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_2">
                <FrameLayout android:id="@+id/frameStackjudulinput"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_90pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_33pxv" android:layout_width="@dimen/_325pxh">
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinput"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJudulInput"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{signUpVM.signUpModel.txtJudulInput}"
                            style="@style/txtInterbold13" tools:text="@string/lbl_nama"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_100_a2_radius_8"
                            android:ellipsize="end"
                            android:hint="@string/msg_masukkan_nama_a"
                            android:id="@+id/etFrameOne"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_9pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@={signUpVM.signUpModel.etFrameOneValue}"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <Spinner
                        android:background="@drawable/layer_list_bg_bluegray_100_border_black_900_radius_8"
                        android:id="@+id/spinnerFrameTen"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackjudulinputOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_110pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_324pxh">
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinputOne"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJudulInputOne"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{signUpVM.signUpModel.txtJudulInputOne}"
                            style="@style/txtInterbold13" tools:text="@string/lbl_e_mail"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumninputtext"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylebluegray_100_a2cornerRadius">
                            <TextView android:ellipsize="end"
                                android:gravity="start"
                                android:id="@+id/txtInputText"
                                android:layout_height="match_parent"
                                android:layout_marginBottom="@dimen/_9pxv"
                                android:layout_marginEnd="@dimen/_18pxh"
                                android:layout_marginStart="@dimen/_18pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{signUpVM.signUpModel.txtInputText}"
                                style="@style/txtInterthin12" tools:text="@string/lbl_masukkan_e_mail2"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinputTwo"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJudulInputTwo"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{signUpVM.signUpModel.txtJudulInputTwo}"
                            style="@style/txtInterbold13" tools:text="@string/lbl_nomor_telepon"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_100_a2_radius_8"
                            android:ellipsize="end"
                            android:hint="@string/lbl_nomor_telepon"
                            android:id="@+id/etFrameOneOne"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@={signUpVM.signUpModel.etFrameOneOneValue}"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                    </LinearLayout>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJudulInputThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtJudulInputThree}"
                    style="@style/txtInterbold13" tools:text="@string/lbl_kata_sandi"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_100_a2_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/lbl_kata_sandi_anda"
                    android:id="@+id/etFrameOneTwo"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_18pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@={signUpVM.signUpModel.etFrameOneTwoValue}"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJudulInputFour"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtJudulInputFour}"
                    style="@style/txtInterbold13" tools:text="@string/msg_konfirmasi_kata"/>
                <EditText
                    android:background="@drawable/rectangle_bg_bluegray_100_a2_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/msg_konfirmasi_kata2"
                    android:id="@+id/etFrameOneThree"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_18pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@={signUpVM.signUpModel.etFrameOneThreeValue}"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnDaftar"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_38pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_38pxv"
                    android:layout_width="@dimen/_150pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_daftar"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_3" tools:text="@string/lbl_daftar"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtMasukAkunRefe"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtMasukAkunRefe}"
                    style="@style/txtInterregular12" tools:text="@string/msg_sudah_punya_aku"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
