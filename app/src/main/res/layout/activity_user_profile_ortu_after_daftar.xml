<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="userProfileOrtuAfterDaftarVM" type="com.application.logicliberators.modules.userprofileortuafterdaftar.data.viewmodel.UserProfileOrtuAfterDaftarVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_A200">
        <androidx.appcompat.widget.Toolbar android:gravity="end"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameButtonback"
                    android:layout_height="@dimen/_10pxv"
                    android:layout_marginBottom="@dimen/_68pxv"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_5pxh" style="@style/groupStylecornerRadius_3">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageFotoprofil"
                    android:layout_height="@dimen/_83pxh"
                    android:layout_marginStart="@dimen/_89pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_83pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_fotoprofil}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_fotoprofil"/>
                <ImageView android:id="@+id/imageSettings"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginBottom="@dimen/_63pxv"
                    android:layout_marginStart="@dimen/_95pxh"
                    android:layout_width="@dimen/_21pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnsriirawati"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleindigo_A200">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSriIrawati"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_75pxh"
                android:layout_marginStart="@dimen/_75pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtSriIrawati}"
                style="@style/txtPoppinsbold14" tools:text="@string/lbl_sri_irawati"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtTitle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_75pxh"
                android:layout_marginStart="@dimen/_75pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtTitle}"
                style="@style/txtPoppinsregular11" tools:text="@string/msg_bergabung_sejak"/>
            <LinearLayout android:id="@+id/linearColumncall"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_100cornerRadius">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcall"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_44pxh"
                    android:layout_marginStart="@dimen/_44pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCall"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_call"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt628137xxxx525"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txt628137xxxx525}"
                        style="@style/txtPoppinssemibold15" tools:text="@string/lbl_62_8137xxxx525"/>
                </LinearLayout>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearRowlocation"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLocation"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_location"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                    <TextView android:id="@+id/txtJlnBumiCender"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_weight="0.91"
                        android:layout_width="0dp"
                        android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtJlnBumiCender}"
                        style="@style/txtPoppinsmediumitalic12" tools:text="@string/msg_jln_bumi_cender"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtBtn"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtBtn}"
                    style="@style/txtSolidRounded" tools:text="@string/msg_terakhir_diliha"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearTawarandaftar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_6pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylelight_blue_800_3fcornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDaftarMenjadi"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_79pxh"
                        android:layout_marginStart="@dimen/_79pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtDaftarMenjadi}"
                        style="@style/txtPoppinsbold14_1" tools:text="@string/msg_daftar_menjadi"/>
                    <TextView android:gravity="start"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_41pxh"
                        android:layout_marginStart="@dimen/_41pxh"
                        android:layout_marginTop="@dimen/_27pxv"
                        android:layout_width="match_parent"
                        android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtLanguage}"
                        style="@style/txtPoppinsmedium14_1" tools:text="@string/msg_berikan_dampak"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtStatus"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_123pxh"
                        android:layout_marginStart="@dimen/_123pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtStatus}"
                        style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_status"/>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearBtndaftar"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_36pxv"
                        android:layout_marginEnd="@dimen/_53pxh"
                        android:layout_marginStart="@dimen/_53pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_5">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtButton"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtButton}"
                            style="@style/txtInterextrabold15" tools:text="@string/msg_menunggu_kon"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumninfo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_83pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_4">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowinfo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="wrap_content"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_4">
                        <ImageView android:id="@+id/imageInfo"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_info"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_info"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAboutUs"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_14pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.73"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtAboutUs}"
                            style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_about_us"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowhelp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_width="wrap_content"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_4">
                        <ImageView android:id="@+id/imageHelp"
                            android:layout_height="@dimen/_22pxh"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_help"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_help"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPusatBantuan"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.8"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtPusatBantuan}"
                            style="@style/txtPoppinsregular12" tools:text="@string/lbl_pusat_bantuan"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowlogout"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_4">
                        <ImageView android:id="@+id/imageLogout"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_logout"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_logout"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtKeluar"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.66"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{userProfileOrtuAfterDaftarVM.userProfileOrtuAfterDaftarModel.txtKeluar}"
                            style="@style/txtPoppinsbold12" tools:text="@string/lbl_keluar"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyleindigo_1">
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_27pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHome"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home_black_900"/>
                <ImageView android:id="@+id/imageRefresh"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_61pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_refresh"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
                <ImageView android:id="@+id/imageLock"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lock"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                <FrameLayout android:id="@+id/frameIconamoonprofi"
                    android:layout_height="@dimen/_33pxv"
                    android:layout_width="@dimen/_33pxh" style="@style/groupStyleindigo_2">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_marginHorizontal="@dimen/_5pxh"
                        android:layout_marginVertical="@dimen/_5pxv"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_gray_105"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_gray_105"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
