<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notifikasiVM" type="com.application.logicliberators.modules.notifikasi.data.viewmodel.NotifikasiVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearNotifikasi"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_12pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_A200">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnkelasberlangsu"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtKelasBerlangsu"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{notifikasiVM.notifikasiModel.txtKelasBerlangsu}"
                style="@style/txtPoppinsextrabold15" tools:text="@string/msg_kelas_berlangsu"/>
            <HorizontalScrollView android:fillViewport="true"
                android:gravity="end"
                android:id="@+id/horizontalScrollHorizontalscrollfotoprofil"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" android:scrollbars="none">
                <LinearLayout android:id="@+id/linearKelasbaru"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
                    <LinearLayout android:id="@+id/linearRowfotoprofil"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.33"
                        android:layout_width="0dp"
                        android:orientation="horizontal" style="@style/groupStylegray_100_b2cornerRadius">
                        <ImageView android:id="@+id/imageFotoprofil"
                            android:layout_height="@dimen/_55pxh"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="@dimen/_55pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_fotoprofil_55x55}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_fotoprofil_55x55"/>
                        <TextView android:id="@+id/txtNamaOrtuSubje"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginEnd="@dimen/_25pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_weight="0.68"
                            android:layout_width="0dp"
                            android:text="@{notifikasiVM.notifikasiModel.txtNamaOrtuSubje}"
                            style="@style/txtPoppinssemibold12" tools:text="@string/msg_puspita_sari_de2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearRowfotoprofilOne"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_weight="0.33"
                        android:layout_width="0dp"
                        android:orientation="horizontal" style="@style/groupStylegray_100_b2cornerRadius">
                        <ImageView android:id="@+id/imageFotoprofilOne"
                            android:layout_height="@dimen/_55pxh"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="@dimen/_55pxh"
                            android:scaleType="centerCrop"
                            circular="@{true}"
                            imageRes="@{@drawable/img_fotoprofil_1}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_fotoprofil_1"/>
                        <TextView android:id="@+id/txtNamaOrtuSubjeOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_25pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.68"
                            android:layout_width="0dp"
                            android:text="@{notifikasiVM.notifikasiModel.txtNamaOrtuSubjeOne}"
                            style="@style/txtPoppinssemibold12" tools:text="@string/msg_puspita_sari_de2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearRowfotoprofilTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_weight="0.33"
                        android:layout_width="0dp"
                        android:orientation="horizontal" style="@style/groupStylegray_100_b2cornerRadius">
                        <ImageView android:id="@+id/imageFotoprofilTwo"
                            android:layout_height="@dimen/_55pxh"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginStart="@dimen/_25pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="@dimen/_55pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_fotoprofil_2}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_fotoprofil_2"/>
                        <TextView android:id="@+id/txtNamaOrtuSubjeTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_25pxh"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.68"
                            android:layout_width="0dp"
                            android:text="@{notifikasiVM.notifikasiModel.txtNamaOrtuSubjeTwo}"
                            style="@style/txtPoppinssemibold12" tools:text="@string/msg_puspita_sari_de2"/>
                    </LinearLayout>
                </LinearLayout>
            </HorizontalScrollView>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnframesixteen"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_105">
                <Spinner
                    android:background="@drawable/layer_list_bg_gray_105_radius_5"
                    android:gravity="start"
                    android:id="@+id/spinnerFrameSixteen"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumndescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleindigo_100_72cornerRadius">
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:text="@{notifikasiVM.notifikasiModel.txtDescription}"
                    style="@style/txtPoppinsbold12" tools:text="@string/msg_pembayaran_anda"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackterimakasihp"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_111pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_317pxh">
                <LinearLayout android:id="@+id/linearColumnterimakasihp"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegray_105cornerRadius">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtTerimaKasihP"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:text="@{notifikasiVM.notifikasiModel.txtTerimaKasihP}"
                        style="@style/txtPoppinsbold12" tools:text="@string/msg_terima_kasih_p"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearColumnselamatpermin"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegray_105cornerRadius">
                    <TextView android:gravity="start"
                        android:id="@+id/txtSelamatPermin"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:text="@{notifikasiVM.notifikasiModel.txtSelamatPermin}"
                        style="@style/txtPoppinsbold12" tools:text="@string/msg_selamat_permin"/>
                </LinearLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/frameStackterimakasiha"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_109pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_316pxh">
                <LinearLayout android:id="@+id/linearColumnterimakasiha"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegray_105cornerRadius">
                    <TextView android:gravity="start"
                        android:id="@+id/txtTerimakasihA"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:text="@{notifikasiVM.notifikasiModel.txtTerimakasihA}"
                        style="@style/txtPoppinsbold12" tools:text="@string/msg_terima_kasih_a"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearColumnterimakasihte"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegray_105cornerRadius">
                    <TextView android:gravity="start"
                        android:id="@+id/txtTerimakasihte"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent"
                        android:text="@{notifikasiVM.notifikasiModel.txtTerimakasihte}"
                        style="@style/txtPoppinsbold12" tools:text="@string/msg_terima_kasih_te"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
