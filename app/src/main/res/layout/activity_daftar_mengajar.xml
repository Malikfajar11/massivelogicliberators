                            android:background="@drawable/rectangle_bg_bluegray_100_a2_radius_8"
                            android:id="@+id/viewFrameOneFour"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_35pxv"
                            android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_324pxh"/>
                    </LinearLayout>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_301_radius_5"
                        android:drawableEnd="@drawable/img_upload"
                        android:drawablePadding="@dimen/_30pxh"
                        android:ellipsize="end"
                        android:hint="@string/lbl_upload_dokumen2"
                        android:id="@+id/etUploaddokumenFour"
