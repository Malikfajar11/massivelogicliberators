                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylelime_600">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJudulInputOne"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{daftarMengajarVM.daftarMengajarModel.txtJudulInputOne}"
                            style="@style/txtInterbold13" tools:text="@string/msg_sertifikat_pend"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_bluegray_100_a2_radius_8"
                            android:ellipsize="end"
                            android:hint="@string/msg_pastikan_data2"
