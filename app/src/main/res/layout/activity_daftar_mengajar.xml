                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                    </LinearLayout>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackjudulinputThree"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_152pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_326pxh">
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinputThree"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylelime_600">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtJudulInputThree"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{daftarMengajarVM.daftarMengajarModel.txtJudulInputThree}"
