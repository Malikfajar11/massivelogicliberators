                                android:layout_gravity="bottom|start"
                                android:layout_height="@dimen/_35pxv"
                                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_324pxh"/>
                            <EditText
                                android:background="@drawable/rectangle_bg_gray_301_radius_5"
                                android:drawableEnd="@drawable/img_upload"
                                android:drawablePadding="@dimen/_30pxh"
                                android:ellipsize="end"
                                android:hint="@string/msg_upload_dokumen"
                                android:id="@+id/etUploaddokumenThree"
                                android:inputType="text"
                                android:layout_gravity="start"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_6pxv"
                                android:paddingEnd="@dimen/_19pxh"
                                android:paddingStart="@dimen/_15pxh"
                                android:paddingTop="@dimen/_12pxv"
                                android:singleLine="true"
                                android:text="@={daftarMengajarVM.daftarMengajarModel.etUploaddokumenThreeValue}"
