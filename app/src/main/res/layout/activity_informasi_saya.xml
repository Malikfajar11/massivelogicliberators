                        <ImageView android:id="@+id/imageFolderTwo"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_9pxv"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_175pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_folder"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_folder"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinputThree"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylelime_600">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtJudulInputThree"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{informasiSayaVM.informasiSayaModel.txtJudulInputThree}"
                            style="@style/txtInterbold13" tools:text="@string/msg_surat_aktif_kul"/>
                        <EditText
                            android:background="@drawable/rectangle_bg_gray_100_a3_radius_8"
                            android:drawableEnd="@drawable/img_edit"
                            android:drawablePadding="@dimen/_30pxh"
                            android:ellipsize="end"
                            android:hint="@string/lbl_asal_kampus"
                            android:id="@+id/etFrameOneTwo"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_18pxh"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_11pxv"
                            android:singleLine="true"
                            android:text="@={informasiSayaVM.informasiSayaModel.etFrameOneTwoValue}"
                            android:textColorHint="@color/black_900"
                            style="@style/etSolidRounded_2" tools:ignore="Autofill"/>
                    </LinearLayout>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackjudulinputFour"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_69pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_324pxh">
                    <LinearLayout
                        android:id="@+id/linearColumnjudulinputFour"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylelime_600">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJudulInputFour"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{informasiSayaVM.informasiSayaModel.txtJudulInputFour}"
