                            android:background="@drawable/rectangle_bg_gray_100_a3_radius_8"
                            android:id="@+id/viewFrameOneThree"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_35pxv"
                            android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_324pxh"/>
                    </LinearLayout>
                    <EditText
                        android:background="@drawable/rectangle_bg_gray_301_radius_5"
                        android:drawableEnd="@drawable/img_folder"
                        android:drawablePadding="@dimen/_30pxh"
                        android:ellipsize="end"
                        android:hint="@string/lbl_dokumen_anda"
                        android:id="@+id/etDokumenOne"
                        android:inputType="text"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_15pxh"
                        android:paddingStart="@dimen/_15pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:singleLine="true"
                        android:text="@={informasiSayaVM.informasiSayaModel.etDokumenOneValue}"
                        android:textColorHint="@color/black_900"
                        style="@style/etSolidRounded_3" tools:ignore="Autofill"/>
                </FrameLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSimpan"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_28pxv"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_45pxv"
                    android:layout_width="@dimen/_93pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_22pxh"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_simpan"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_10" tools:text="@string/lbl_simpan"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
