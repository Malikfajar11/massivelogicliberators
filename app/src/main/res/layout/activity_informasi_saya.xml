                            android:layout_height="@dimen/_18pxh"
                            android:layout_marginBottom="@dimen/_23pxv"
                            android:layout_marginEnd="@dimen/_18pxh"
                            android:layout_marginStart="@dimen/_18pxh"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_edit"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                    </FrameLayout>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackdokumenandaTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_85pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_331pxh">
                    <LinearLayout android:id="@+id/linearDokumen2"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylegray_301cornerRadius">
                        <TextView android:ellipsize="end"
