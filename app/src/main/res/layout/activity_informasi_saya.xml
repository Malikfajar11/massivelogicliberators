                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_width="@dimen/_324pxh" style="@style/groupStylelime_600">
                        <View
                            android:background="@drawable/rectangle_bg_gray_100_a3_radius_8"
                            android:id="@+id/viewFrameOneOne"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_35pxv"
                            android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_324pxh"/>
